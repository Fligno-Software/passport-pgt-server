{
    "name": "fld3/passport-pgt-server",
    "description": "Laravel Passport Password Grant Tokens Server",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarlo.luchavez@fligno.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez"
        }
    ],
    "homepage": "https://github.com/fld3/passport-pgt-server",
    "keywords": ["Laravel", "PassportPgtServer"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~7|~8|~9",
        "laravel/passport": "^10.4",
        "fld3/passport-pgt-client": "^3.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6|~7"
    },
    "autoload": {
        "psr-4": {
            "Fld3\\PassportPgtServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fld3\\PassportPgtServer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fld3\\PassportPgtServer\\Providers\\PassportPgtServerServiceProvider"
            ],
            "aliases": {
                "PassportPgtServer": "Fld3\\PassportPgtServer\\Facades\\PassportPgtServer"
            }
        },
        "branch-alias": {
            "dev-master": "3.0.x-dev",
            "dev-develop": "3.0.x-dev"
        }
    },
    "repositories": {
        "git.fligno.com/440": {
            "type": "composer",
            "url": "https://git.fligno.com/api/v4/group/440/-/packages/composer/packages.json"
        }
    }
}
